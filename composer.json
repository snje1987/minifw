{
    "name": "snje1987/minifw",
    "type": "library",
    "description": "A simple php framework",
    "keywords": ["php", "framework"],
    "homepage": "https://github.com/snje1987/minifw",
    "license": "LGPLv3",
    "authors": [
        {
            "name": "Yang Ming",
            "email": "root@snje.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "gkralik/php-uglifyjs": "dev-master",
        "natxet/cssmin": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "squizlabs/php_codesniffer": "^3.0.2",
        "wimg/php-compatibility": "*"
    },
    "autoload": {
        "psr-4": {
            "Org\\Snje\\Minifw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Org\\Snje\\MinifwTest\\": "tests/"
        }
    }
}