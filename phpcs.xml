<?xml version="1.0"?>
<ruleset name="Minifw">
    <description>Minifw simple standard</description>
    <config name="installed_paths" value="vendor/wimg/php-compatibility" />
    <config name="testVersion" value="5.3-"/>
    <file>src</file>
    <file>tests</file>
    <file>config.sample.php</file>
    <file>index.sample.php</file>
    <arg name="basepath" value="."/>
    <arg name="colors" />
    <arg name="parallel" value="75" />
    <arg value="np"/>
    <rule ref="PHPCompatibility"/>
</ruleset>
